{
  "extends": "./original.tsconfig.json",
  "include": [
    "next-env.d.ts",
    "types",
    "src",
    "pages",
    "data",
    "next.config.mjs"
  ],
  "compilerOptions": {
    "allowJs": true,
    "isolatedModules": true,
    /* files are emitted by babel */
    "noEmit": true,
    "noUnusedLocals": false,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "types": [
      "react",
      "node"
    ],
    "incremental": true,
    "target": "ES2019"
  },
  "exclude": []
}
